                    <DataTemplate>
                        <Grid>
                            <Image Source="Images/HubTileImage.png"/>
                        </Grid>
                    </DataTemplate>
                </telerikPrimitives:RadCustomHubTile.FrontContentTemplate>
            </telerikPrimitives:RadCustomHubTile>
        </DataTemplate>

    </UserControl.Resources>

